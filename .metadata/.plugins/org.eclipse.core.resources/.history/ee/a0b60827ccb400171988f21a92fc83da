package br.com.fiap.conexao;

import java.sql.Connection;

public class TesteConexao {

	public static void main(String[] args) {
	
		Connection c = null;

		try{
			c = new ConexaoFactory().connectar();
			
		}catch(Exception e){
			e.printStackTrace();
		}finally{
			try{
				c.close();
			}catch(Exception e){
				e.printStackTrace();
			}
		}
		
	}

}
