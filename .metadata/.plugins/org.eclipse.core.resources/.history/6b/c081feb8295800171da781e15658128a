package br.com.fiap.conexao;

import java.sql.Connection;
import java.sql.DriverManager;

public class ConexaoFactory {

	
	//Classe com uma função de fabricar a conexão
	
	
	public Connection queroConexao()throws Exception{
		
		return DriverManager.getConnection("jdbc:oracle:thin:@oracle.fiap.com.br:1521:ORCL");
		//Minha casa "jdbc:oracle:thin:@localhost:1521:xe";
		
	
	}
	
	
}
