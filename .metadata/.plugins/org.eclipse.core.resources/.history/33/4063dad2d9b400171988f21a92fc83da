package br.com.fiap.bean;

import java.io.Serializable;

public class Curso implements Comparable<Curso> , Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 8417808940489848194L;
	
	private int cd_curso;
	private String nm_curso;
	private double vl_curso;
	private String ds_titulacao;
	
	
	public int compareTo(Curso o) {
		return this.nm_curso.compareTo(o.nm_curso);
		}

	

	public Curso(int cd_curso, String nm_curso, double vl_curso, String ds_titulacao) {
		super();
		setCd_curso(cd_curso);
		setDs_titulacao(ds_titulacao);
		setNm_curso(nm_curso);
		setVl_curso(vl_curso);
	}


	public int getCd_curso() {
		return cd_curso;
	}


	public void setCd_curso(int cd_curso) {
		this.cd_curso = cd_curso;
	}


	public String getNm_curso() {
		return nm_curso;
	}


	public void setNm_curso(String nm_curso) {
		this.nm_curso = nm_curso;
	}


	public double getVl_curso() {
		return vl_curso;
	}


	public void setVl_curso(double vl_curso) {
		this.vl_curso = vl_curso;
	}


	public String getDs_titulacao() {
		return ds_titulacao;
	}


	public void setDs_titulacao(String ds_titulacao) {
		this.ds_titulacao = ds_titulacao;
	}
	
	
	
	
	
	
	
	
}
