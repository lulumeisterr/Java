package br.com.fiap.bo;

import java.util.List;

import br.com.fiap.bean.Disciplina;
import br.com.fiap.dao.DisciplinaDAO;


public class DisciplinaBO {
	

	public static String gravarDisciplina(Disciplina d , int fk_curso)throws Exception{
		
			DisciplinaDAO dis = new DisciplinaDAO();
			List<Estado> e = dao.consultaPorSigla(s);
			dao.fechar();
			return e;
		}
		
		
	}

}
